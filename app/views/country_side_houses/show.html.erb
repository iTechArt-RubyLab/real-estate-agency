<p>
  <% if @country_side_house.lot.images.attached? %>
    <% @country_side_house.lot.images.each do |image| %>
      <%= image_tag image.variant(resize_to_limit: [250, 250]).processed %>
    <% end %>
  <% end %>
</p>

<p style="text-align: center; font-size: 20px">
  <strong>Title:</strong>
  <%= @country_side_house.lot.title %>
</p>

<p style="text-align: center; font-size: 20px">
  <strong>Description:</strong>
  <%= @country_side_house.lot.description %>
</p>

<p style="text-align: center; font-size: 20px">
  <strong>Price:</strong>
  <%= @country_side_house.lot.price %>
</p>

<p style="text-align: center; font-size: 20px">
  <strong>Deal type:</strong>
  <%= @country_side_house.lot.deal_type.name %>
</p>

<p style="text-align: center; font-size: 20px">
  <strong>City:</strong>
  <%= @country_side_house.lot.address.street.district.city.name %>
</p>

<p style="text-align: center; font-size: 20px">
  <strong>District:</strong>
  <%= @country_side_house.lot.address.street.district.name %>
</p>

<p style="text-align: center; font-size: 20px">
  <strong>Street:</strong>
  <%= @country_side_house.lot.address.street.name %>
</p>

<p style="text-align: center; font-size: 20px">
  <strong>Building:</strong>
  <%= @country_side_house.lot.address.building %>
</p>

<p style="text-align: center; font-size: 20px">
  <strong>Address description:</strong>
  <%= @country_side_house.lot.address.description %>
</p>

<p style="text-align: center; font-size: 20px">
  <strong>Kind:</strong>
  <%= @country_side_house.country_side_house_kind.name %>
</p>

<p style="text-align: center; font-size: 20px">
  <strong>Ready state:</strong>
  <%= @country_side_house.ready_state.name %>
</p>

<p style="text-align: center; font-size: 20px">
  <strong>Floors count:</strong>
  <%= @country_side_house.floors_count %>
</p>

<p style="text-align: center; font-size: 20px">
  <strong>Land area:</strong>
  <%= @country_side_house.land_area %>
</p>

<p style="text-align: center; font-size: 20px">
  <strong>Total area:</strong>
  <%= @country_side_house.total_area %>
</p>

<p style="text-align: center; font-size: 20px">
  <strong>Year of construction:</strong>
  <%= @country_side_house.year_of_construction %>
</p>

<p style="text-align: center; font-size: 20px">
  <strong>Wall material:</strong>
  <%= @country_side_house.wall_material.name %>
</p>

<% if current_user.profilable_type = 'RealtorProfile' %>
  <p style="text-align: center; font-size: 20px">
    <strong>Asigner:</strong>
    <% if @country_side_house.lot.asigner.nil? %>
      <%= 'Asigner not match' %>
    <% else %>
      <%= @country_side_house.lot.asigner.email %>
    <% end %>
  </p>

  <p style="text-align: center; font-size: 20px">
    <strong>Asignee:</strong>
    <% if @country_side_house.lot.asignee.nil? %>
      <%= 'Asignee not match' %>
    <% else %>
      <%= @country_side_house.lot.asignee.email %>
    <% end %>
  </p>

  <p style="text-align: center; font-size: 20px">
    <strong>Client:</strong>
    <% if @country_side_house.lot.client.nil? %>
      <%= 'Client not match' %>
    <% else %>
      <%= @country_side_house.lot.client.email %>
    <% end %>
  </p>
<% end %>

<%= link_to 'Edit', edit_country_side_house_path(@country_side_house), class: "btn btn-warning" %>
<%= link_to 'Back', country_side_houses_path, class: "btn btn-danger" %>
