<p id="notice"><%= notice %></p>

<h1>Changes Histories</h1>

<table>
  <thead>
    <tr>
      <th>Changes</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @changes_histories.each do |changes_history| %>
      <tr>
        <td><%= changes_history.changes %></td>
        <td><%= changes_history.user_id %></td>
        <td><%= link_to 'Show', changes_history %></td>
        <td><%= link_to 'Edit', edit_changes_history_path(changes_history) %></td>
        <td><%= link_to 'Destroy', changes_history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Changes History', new_changes_history_path %>
